;; Test03
;; tabuleiro vazio
;; Colocar 90 pecas

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))
(ignore-value (setf lista-pecas '(z s o l j i i s j s z z i o t t l i t s z o i l s t o l i o s t t o l l i s t o l s s t t i o l l l s l o t i o i i o t o i t l i t l o i t t s s t i l o t i t l s l t i i t t i l )))

(time (procura-best (tabuleiro-data t1) lista-pecas))

(with-open-file (str "test03/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) lista-pecas 'IGNORE)))