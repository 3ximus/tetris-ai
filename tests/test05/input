;; Test05

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))

;;deve retornar IGNORE
(time (procura-best (tabuleiro-data t1) '(i o t l z)))

(with-open-file (str "test05/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) '(i o t l z) 'IGNORE)))