;; Test20

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))

;;deve retornar IGNORE
(time (procura-best (tabuleiro-data t1) '(t l i l l o s s i i o o i s o s t t t z)))

(with-open-file (str "test20/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) '(t l i l l o s s i i o o i s o s t t t z) 'IGNORE)))