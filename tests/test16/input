;; Test16

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))

;;deve retornar IGNORE
(time (procura-best (tabuleiro-data t1) '(s z i j s l l z i l i o s z o l)))

(with-open-file (str "test16/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) '(s z i j s l l z i l i o s z o l) 'IGNORE)))