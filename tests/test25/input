;; Test25

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))

;;deve retornar IGNORE
(time (procura-best (tabuleiro-data t1) '(l i j i t t l j i t l j s l s j t s i s l o i z t)))

(with-open-file (str "test25/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) '(l i j i t t l j i t l j s l s j t s i s l o i z t) 'IGNORE)))