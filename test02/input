;; Test02
;; tabuleiro vazio

;;deve retornar IGNORE
(ignore-value (setf t1 (cria-tabuleiro)))

(time (procura-best (tabuleiro-data t1) '(z s o l j i i s)))

(with-open-file (str "test02/output.txt"
                     :direction :output
                     :if-exists :supersede
                     :if-does-not-exist :create)
(format str "~S~%" (procura-best (tabuleiro-data t1) '(z s o l j i i s) 'IGNORE)))